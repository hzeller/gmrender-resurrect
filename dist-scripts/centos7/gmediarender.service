[Unit]
Description=GMediaRender UPnP/DLNA renderer
After=network.target sound.target

[Service]
Type=simple
User=gmediarender
Group=gmediarender
PIDFile=/run/gmediarender.pid
ExecStart=/usr/bin/gmediarender --daemon --pid-file=/run/gmediarender.pid --port=49494

[Install]
WantedBy=multi-user.target
